{
  "kind": "collectionType",
  "collectionName": "projects",
  "info": {
    "name": "Project"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "description": {
      "type": "richtext"
    },
    "images": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "cover": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "service": {
      "model": "service",
      "via": "projects"
    },
    "client": {
      "model": "client",
      "via": "projects"
    },
    "testimonial": {
      "via": "project",
      "model": "testimonial"
    }
  }
}
